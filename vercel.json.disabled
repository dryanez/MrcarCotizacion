{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python",
            "config": {
                "includeFiles": [
                    "index.html",
                    "static/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "app.py"
        },
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ]
}